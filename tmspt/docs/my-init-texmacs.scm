(set-font-rules
 '(
    ((Times rm medium right $s $d) (ec ec-Times-medium-right $s $d))
    ((Times rm medium slanted $s $d) (ec ec-Times-medium-slanted $s $d))
    ((Times rm medium italic $s $d) (ec ec-Times-medium-italic $s $d))
    ((Times rm medium left-slanted $s $d) (ec ecff $s $d))
    ((Times rm medium small-caps $s $d) (ec ec-Times-medium-smallcaps $s $d))
    ((Times rm medium long $s $d) (ec ecdh $s $d))
    ((Times rm medium italic-right $s $d) (ec ecui $s $d))
    ((Times rm bold right $s $d) (ec ec-Times-bold-right $s $d))
    ((Times rm bold slanted $s $d) (ec ec-Times-bold-slanted $s $d))
    ((Times rm bold italic $s $d) (ec ec-Times-bold-italic $s $d))
    ((Times rm bold condensed $s $d) (ec ecrb $s $d))
    ((Times rm bold small-caps $s $d) (ec ec-Times-bold-smallcaps $s $d))
    ((Times rm bold slanted-small-caps $s $d) (ec ecoc $s $d))
    
    ((alextest rm medium right $s $d) (ec alextest $s $d))
    
    ((Times tt medium right $s $d) (ec ec-Courier-medium-right $s $d))
    ((Times tt medium slanted $s $d) (ec ecst $s $d))
    ((Times tt medium italic $s $d) (ec ec-Courier-medium-italic $s $d))
    ((Times tt medium small-caps $s $d) (ec ectc $s $d))
    ((Times tt medium flat $s $d) (ec ecltt $s $d))
    ((Times tt medium proportional $s $d) (ec ecvt $s $d))
    ((Times tt medium italic-proportional $s $d) (ec ecvi $s $d))
    ((Times tt bold right $s $d) (ec ec-Courier-bold-right $s $d))
    
    ((Times ss medium right $s $d) (ec ec-Helvetica-medium-right $s $d))
    ((Times ss medium slanted $s $d) (ec ecsi $s $d))
    ((Times ss medium italic $s $d) (ec ec-Helvetica-medium-italic $s $d))
    ((Times ss medium flat $s $d) (ec eclq $s $d 8))`
    ((Times ss medium italic-flat $s $d) (ec ecli $s $d 8))
    ((Times ss bold right $s $d) (ec ec-Helvetica-bold-right $s $d))
    ((Times ss bold slanted $s $d) (ec ecso $s $d))
    ((Times ss bold italic $s $d) (ec ec-Helvetica-bold-italict $s $d))
    ((Times ss bold condensed $s $d) (ec ecssdc $s $d))
    ((Times ss bold flat $s $d) (ec eclb $s $d 8))
    ((Times ss bold italic-flat $s $d) (ec eclo $s $d 8))
    
    
    ((Palatino rm medium right $s $d) (ec ec-Palatino-medium-right $s $d))
    ((Palatino rm medium slanted $s $d) (ec ec-Palatino-medium-slanted $s $d))
    ((Palatino rm medium italic $s $d) (ec ec-Palatino-medium-italic $s $d))
    ((Palatino rm medium small-caps $s $d) (ec ec-Palatino-medium-smallcaps $s $d))
    ((Palatino rm bold right $s $d) (ec ec-Palatino-bold-right $s $d))
    ((Palatino rm bold slanted $s $d) (ec ec-Palatino-bold-slanted $s $d))
    ((Palatino rm bold italic $s $d) (ec ec-Palatino-bold-italic $s $d))
    ((Palatino rm bold small-caps $s $d) (ec ec-Palatino-bold-smallcaps $s $d))
    
    ((Palatino tt $a $b $s $d) (Times tt $a $b $s $d))
    ((Palatino ss $a $b $s $d) (Times ss $a $b $s $d))
    
    ((AvantGarde rm medium right $s $d) (ec ec-AvantGarde-medium-right $s $d))
    ((AvantGarde rm medium italic $s $d) (ec ec-AvantGarde-medium-italic $s $d))
    ((AvantGarde rm bold right $s $d) (ec ec-AvantGarde-bold-right $s $d))
    ((AvantGarde rm bold italic $s $d) (ec ec-AvantGarde-bold-italic $s $d))
    
    ((Bookman rm medium right $s $d) (ec ec-Bookman-medium-right $s $d))
    ((Bookman rm medium italic $s $d) (ec ec-Bookman-medium-italic $s $d))
    ((Bookman rm bold right $s $d) (ec ec-Bookman-bold-right $s $d))
    ((Bookman rm bold italic $s $d) (ec ec-Bookman-bold-italic $s $d))
    
    ((NewCenturySchlbk rm medium right $s $d) (ec ec-NewCenturySchlbk-medium-right $s $d))
    ((NewCenturySchlbk rm medium italic $s $d) (ec ec-NewCenturySchlbk-medium-italic $s $d))
    ((NewCenturySchlbk rm bold right $s $d) (ec ec-NewCenturySchlbk-bold-right $s $d))
    ((NewCenturySchlbk rm bold italic $s $d) (ec ec-NewCenturySchlbk-bold-italic $s $d))
    
    ((DejaVuSerif rm medium right $s $d) (ec ec-DejaVuSerif-medium-right $s $d))
    ((DejaVuSerif rm medium italic $s $d) (ec ec-DejaVuSerif-medium-italic $s $d))
    ((DejaVuSerif rm bold right $s $d) (ec ec-DejaVuSerif-bold-right $s $d))
    ((DejaVuSerif rm bold italic $s $d) (ec ec-DejaVuSerif-bold-italic $s $d))
    
    ((DejaVuSans rm medium right $s $d) (ec ec-DejaVuSans-medium-right $s $d))
    ((DejaVuSans rm medium italic $s $d) (ec ec-DejaVuSans-medium-italic $s $d))
    ((DejaVuSans rm bold right $s $d) (ec ec-DejaVuSans-bold-right $s $d))
    ((DejaVuSans rm bold italic $s $d) (ec ec-DejaVuSans-bold-italic $s $d))
    
    ((DejaVuSansMono rm medium right $s $d) (ec ec-DejaVuSansMono-medium-right $s $d))
    ((DejaVuSansMono rm medium italic $s $d) (ec ec-DejaVuSansMono-medium-italic $s $d))
    ((DejaVuSansMono rm bold right $s $d) (ec ec-DejaVuSansMono-bold-right $s $d))
    ((DejaVuSansMono rm bold italic $s $d) (ec ec-DejaVuSansMono-bold-italic $s $d))
    
    ((LiberationSerif rm medium right $s $d) (ec ec-LiberationSerif-medium-right $s $d))
    ((LiberationSerif rm medium italic $s $d) (ec ec-LiberationSerif-medium-italic $s $d))
    ((LiberationSerif rm bold right $s $d) (ec ec-LiberationSerif-bold-right $s $d))
    ((LiberationSerif rm bold italic $s $d) (ec ec-LiberationSerif-bold-italic $s $d))
    
    ((LiberationSans rm medium right $s $d) (ec ec-LiberationSans-medium-right $s $d))
    ((LiberationSans rm medium italic $s $d) (ec ec-LiberationSans-medium-italic $s $d))
    ((LiberationSans rm bold right $s $d) (ec ec-LiberationSans-bold-right $s $d))
    ((LiberationSans rm bold italic $s $d) (ec ec-LiberationSans-bold-italic $s $d))
    
    ((LiberationMono rm medium right $s $d) (ec ec-LiberationSansMono-medium-right $s $d))
    ((LiberationMono rm medium italic $s $d) (ec ec-LiberationSansMono-medium-italic $s $d))
    ((LiberationMono rm bold right $s $d) (ec ec-LiberationSansMono-bold-right $s $d))
    ((LiberationMono rm bold italic $s $d) (ec ec-LiberationSansMono-bold-italic $s $d))
    
    ((Helvetica rm medium right $s $d) (ec ec-Helvetica-medium-right $s $d))
    ((Helvetica rm medium italic $s $d) (ec ec-Helvetica-medium-italic $s $d))
    ((Helvetica rm bold right $s $d) (ec ec-Helvetica-bold-right $s $d))
    ((Helvetica rm bold italic $s $d) (ec ec-Helvetica-bold-italic $s $d))
    
    ((Courier rm medium right $s $d) (ec ec-Courier-medium-right $s $d))
    ((Courier rm medium italic $s $d) (ec ec-Courier-medium-italic $s $d))
    ((Courier rm bold right $s $d) (ec ec-Courier-bold-right $s $d))
    ((Courier rm bold italic $s $d) (ec ec-Courier-bold-italic $s $d))
    
    ((Times mr medium $a $s $d) (math-std ec-Times-medium-right cm-Times-medium-right math-italic-Times-medium $s $d))
    ((Times mr bold $a $s $d) (bold-math-std ec-Times-bold-right cm-Times-bold-right math-italic-Times-bold $s $d))
    
    ((Palatino mr medium $a $s $d) (math-std ec-Palatino-medium-right cm-Palatino-medium-right math-italic-Palatino-medium $s $d))
    ((Palatino mr bold $a $s $d) (bold-math-std ec-Palatino-bold-right cm-Palatino-bold-right math-italic-Palatino-bold $s $d))
   
    ((DejaVuSerif mr medium $a $s $d) (math ec-DejaVuSerif-medium-right cm-DejaVuSerif-medium-right math-italic-DejaVuSerif-medium math-symbol-DejaVuSerif-medium 
                        msam msbm stmary wasy line cmsy eufm bbm grmn cmbx cmmib cmbsy cmex $s $d))                     
    ((DejaVuSerif mr bold $a $s $d) (math ec-DejaVuSerif-bold-right cm-DejaVuSerif-bold-right math-italic-DejaVuSerif-bold math-symbol-DejaVuSerif-bold 
                        msam msbm stmaryb wasyb linew cmbsy eufb bbmbx grxn cmbx cmmib cmbsy cmex $s $d))
    
   
    
    ((LiberationSerif mr medium $a $s $d) (math ec-LiberationSerif-medium-right cm-LiberationSerif-medium-right math-italic-LiberationSerif-medium math-symbol-LiberationSerif-medium 
                        msam msbm stmary wasy line cmsy eufm bbm grmn cmbx cmmib cmbsy cmex $s $d))
    
    ((LiberationSerif mr bold $a $s $d) (math ec-LiberationSerif-bold-right cm-LiberationSerif-bold-right math-italic-LiberationSerif-bold math-symbol-LiberationSerif-bold 
                        msam msbm stmaryb wasyb linew cmbsy eufb bbmbx grxn cmbx cmmib cmbsy cmex $s $d))))
(menu-extend texmacs-extra-menu
    (=> "FreeFonts"
      (-> "Document Font"
        ("Times" 
            (begin (init-env "font" "Times")
                   (init-env "prog-font" "Times")
                   (init-env "math-font" "Times")))

             
        ("Palatino" 
            (begin (init-env "font" "Palatino")
                   (init-env "prog-font" "Palatino")
                   (init-env "math-font" "Palatino")))
        
        ("DejaVuSerif"
            (begin (init-env "font" "DejaVuSerif")
                   (init-env "prog-font" "DejaVuSerif")
                   (init-env "math-font" "DejaVuSerif")))
       
        ("LiberationSerif"
            (begin (init-env "font" "LiberationSerif")
                   (init-env "prog-font" "LiberationSerif")
                   (init-env "math-font" "LiberationSerif"))))

                   
      (-> "Current Font"
          ("Times"
            (begin (make-with "font" "Times")))
            
          ("Helvetica"
            (begin (make-with "font" "Helvetica")))
            
          ("Courier"
            (begin (make-with "font" "Courier")))
            
          ("DejaVu Serif"
            (begin (make-with "font" "DejaVuSerif")))

          ("DejaVu Sans"
            (begin (make-with "font" "DejaVuSans")))
       
          ("DejaVu Mono"
            (begin (make-with "font" "DejaVuSansMono")))

          ("Palatino"
            (begin (make-with "font" "Palatino")))

          ("AvantGarde"
            (begin (make-with "font" "AvantGarde")))
            
          ("Bookman"
            (begin (make-with "font" "Bookman")))
          
          ("New Century Schoolbook"
            (begin (make-with "font" "NewCenturySchlbk"))) 

          ("Liberation Serif"
            (begin (make-with "font" "LiberationSerif"))) 
          
          ("Liberation Sans"
            (begin (make-with "font" "LiberationSans")))
          
          ("Liberation Mono"
            (begin (make-with "font" "LiberationMono"))))))

