(cl:declaim (cl:optimize cl:debug cl:safety))
(cl:declaim (sb-ext:muffle-conditions sb-ext:compiler-note cl:style-warning))
(MODULE (LIST 'XVECT)) 
(PUT 'MKXVECT 'NUMBER-OF-ARGS 0) 
(PUT 'MKXVECT 'DEFINED-ON-LINE '32) 
(PUT 'MKXVECT 'DEFINED-IN-FILE 'PLOT/XVECT.RED) 
(PUT 'MKXVECT 'PROCEDURE_TYPE '(ARROW UNIT GENERAL)) 
(DE MKXVECT NIL (LIST (MKVECT 1024))) 
(PUT 'XPUTV 'NUMBER-OF-ARGS 3) 
(PUT 'XPUTV 'DEFINED-ON-LINE '34) 
(PUT 'XPUTV 'DEFINED-IN-FILE 'PLOT/XVECT.RED) 
(PUT 'XPUTV 'PROCEDURE_TYPE '(ARROW (TIMES GENERAL GENERAL GENERAL) GENERAL)) 
(DE XPUTV (V N W)
    (PROG (I J)
      (SETQ I (IQUOTIENT N 1024))
      (SETQ J (IREMAINDER N 1024))
      (PROG ()
       WHILELABEL
        (COND ((NOT (GREATERP I 0)) (RETURN NIL)))
        (PROGN
         (COND ((NULL (CDR V)) (SETCDR V (MKXVECT))))
         (SETQ V (CDR V))
         (SETQ I (IDIFFERENCE I 1)))
        (GO WHILELABEL))
      (IPUTV (CAR V) J W)
      (RETURN W))) 
(PUT 'XGETV 'NUMBER-OF-ARGS 2) 
(PUT 'XGETV 'DEFINED-ON-LINE '44) 
(PUT 'XGETV 'DEFINED-IN-FILE 'PLOT/XVECT.RED) 
(PUT 'XGETV 'PROCEDURE_TYPE '(ARROW (TIMES GENERAL GENERAL) GENERAL)) 
(DE XGETV (V N)
    (PROG (I J W)
      (SETQ I (IQUOTIENT N 1024))
      (SETQ J (IREMAINDER N 1024))
      (PROG ()
       WHILELABEL
        (COND ((NOT (AND (GREATERP I 0) V)) (RETURN NIL)))
        (PROGN (SETQ V (CDR V)) (SETQ I (IDIFFERENCE I 1)))
        (GO WHILELABEL))
      (SETQ W (COND (V (IGETV (CAR V) J))))
      (RETURN W))) 
(ENDMODULE) 