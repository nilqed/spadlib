(PUT 'ACFSF_SUBSUMPTION 'NUMBER-OF-ARGS 3) 
(PUT 'ACFSF_SUBSUMPTION 'DEFINED-ON-LINE '50) 
(PUT 'ACFSF_SUBSUMPTION 'DEFINED-IN-FILE 'REDLOG/ACFSF/ACFSFBNF.RED) 
(PUT 'ACFSF_SUBSUMPTION 'PROCEDURE_TYPE
     '(ARROW (TIMES GENERAL GENERAL GENERAL) GENERAL)) 
(DE ACFSF_SUBSUMPTION (L1 L2 GOR)
    (COND
     ((EQ GOR 'OR)
      (COND ((ACFSF_SUBSUMEP-AND L1 L2) 'KEEP2)
            ((ACFSF_SUBSUMEP-AND L2 L1) 'KEEP1)))
     ((ACFSF_SUBSUMEP-OR L1 L2) 'KEEP1) ((ACFSF_SUBSUMEP-OR L2 L1) 'KEEP2))) 
(PUT 'ACFSF_SUBSUMEP-AND 'NUMBER-OF-ARGS 2) 
(PUT 'ACFSF_SUBSUMEP-AND 'DEFINED-ON-LINE '65) 
(PUT 'ACFSF_SUBSUMEP-AND 'DEFINED-IN-FILE 'REDLOG/ACFSF/ACFSFBNF.RED) 
(PUT 'ACFSF_SUBSUMEP-AND 'PROCEDURE_TYPE
     '(ARROW (TIMES GENERAL GENERAL) GENERAL)) 
(DE ACFSF_SUBSUMEP-AND (L1 L2)
    (PROG (A)
      (PROG ()
       WHILELABEL
        (COND ((NOT L2) (RETURN NIL)))
        (PROGN
         (SETQ A (CAR L2))
         (SETQ L2 (CDR L2))
         (COND ((NEQ (CL_SIMPL A L1 (MINUS 1)) 'TRUE) (SETQ A (SETQ L2 NIL)))))
        (GO WHILELABEL))
      (RETURN A))) 
(PUT 'ACFSF_SUBSUMEP-OR 'NUMBER-OF-ARGS 2) 
(PUT 'ACFSF_SUBSUMEP-OR 'DEFINED-ON-LINE '77) 
(PUT 'ACFSF_SUBSUMEP-OR 'DEFINED-IN-FILE 'REDLOG/ACFSF/ACFSFBNF.RED) 
(PUT 'ACFSF_SUBSUMEP-OR 'PROCEDURE_TYPE
     '(ARROW (TIMES GENERAL GENERAL) GENERAL)) 
(DE ACFSF_SUBSUMEP-OR (L1 L2)
    (PROG (A)
      (PROG ()
       WHILELABEL
        (COND ((NOT L1) (RETURN NIL)))
        (PROGN
         (SETQ A (CAR L1))
         (SETQ L1 (CDR L1))
         (COND
          ((NEQ
            (CL_SIMPL
             (COND ((AND L2 (CDR L2)) (CONS 'OR L2))
                   ((NULL L2) (COND ((EQ 'OR 'AND) 'TRUE) (T 'FALSE)))
                   (T (CAR L2)))
             (LIST A) (MINUS 1))
            'TRUE)
           (SETQ A (SETQ L1 NIL)))))
        (GO WHILELABEL))
      (RETURN A))) 
(PUT 'ACFSF_SACATLP 'NUMBER-OF-ARGS 2) 
(PUT 'ACFSF_SACATLP 'DEFINED-ON-LINE '89) 
(PUT 'ACFSF_SACATLP 'DEFINED-IN-FILE 'REDLOG/ACFSF/ACFSFBNF.RED) 
(PUT 'ACFSF_SACATLP 'PROCEDURE_TYPE '(ARROW (TIMES GENERAL GENERAL) GENERAL)) 
(DE ACFSF_SACATLP (A L)
    ((LAMBDA (W)
       (NOT
        (AND (NEQ (ACFSF_ARG2L A) (ACFSF_ARG2L W))
             (ORDP (ACFSF_ARG2L A) (ACFSF_ARG2L W)))))
     (CAR L))) 
(PUT 'ACFSF_SACAT 'NUMBER-OF-ARGS 3) 
(PUT 'ACFSF_SACAT 'DEFINED-ON-LINE '97) 
(PUT 'ACFSF_SACAT 'DEFINED-IN-FILE 'REDLOG/ACFSF/ACFSFBNF.RED) 
(PUT 'ACFSF_SACAT 'PROCEDURE_TYPE
     '(ARROW (TIMES GENERAL GENERAL GENERAL) GENERAL)) 
(DE ACFSF_SACAT (A1 A2 GOR)
    (PROG (W)
      (COND ((NEQ (ACFSF_ARG2L A1) (ACFSF_ARG2L A2)) (RETURN NIL)))
      (SETQ W (ACFSF_SACREL (ACFSF_OP A1) (ACFSF_OP A2) GOR))
      (COND ((MEMQ W '(DROP KEEP KEEP1 KEEP2)) (RETURN W)))
      (RETURN (ACFSF_0MK2 W (ACFSF_ARG2L A1))))) 
(PUT 'ACFSF_SACREL 'NUMBER-OF-ARGS 3) 
(PUT 'ACFSF_SACREL 'DEFINED-ON-LINE '116) 
(PUT 'ACFSF_SACREL 'DEFINED-IN-FILE 'REDLOG/ACFSF/ACFSFBNF.RED) 
(PUT 'ACFSF_SACREL 'PROCEDURE_TYPE
     '(ARROW (TIMES GENERAL GENERAL GENERAL) GENERAL)) 
(DE ACFSF_SACREL (R1 R2 GOR) (COND ((EQ R1 R2) 'KEEP) (T 'DROP))) 
(ENDMODULE) 