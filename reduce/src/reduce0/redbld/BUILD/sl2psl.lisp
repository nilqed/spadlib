     (SETQ ELM (CONS ELM NIL))
     (COND ((NULL L) (NCONC (LIST ELM) ELM))
           ((NULL (CAR L)) (RPLACD (RPLACA L ELM) ELM))
           (T (PROGN (RPLACD (CDR L) ELM) (RPLACD L ELM)))))) 
