(cl:declaim (cl:optimize cl:debug cl:safety))
(cl:declaim (sb-ext:muffle-conditions sb-ext:compiler-note cl:style-warning))
(MODULE (LIST 'GOUTPUT)) 
(NULL (SETQ *MODE 'SYMBOLIC)) 
(FLUID '(*ERRCONT)) 
(GLOBAL '(*ERRCHAN* *OUTCHANL* GENTRANLANG* *POSN* *STDIN* *STDOUT* $EOL$)) 
(SETQ *ERRCHAN* NIL) 
(SETQ *POSN* 0) 
(GLOBAL '(*PPRINBUF*)) 
(PUT 'PPRIN2 'NUMBER-OF-ARGS 1) 
(PUT 'PPRIN2 'DEFINED-ON-LINE '56) 
(PUT 'PPRIN2 'DEFINED-IN-FILE 'GENTRAN/GOUTPUT.RED) 
(PUT 'PPRIN2 'PROCEDURE_TYPE '(ARROW GENERAL GENERAL)) 
(DE PPRIN2 (ARG)
    (PROG ()
      (SETQ *PPRINBUF* (CONS ARG *PPRINBUF*))
      (SETQ *POSN* (PLUS *POSN* (LENGTH (EXPLODE2 ARG)))))) 
(PUT 'PTERPRI 'NUMBER-OF-ARGS 0) 
(PUT 'PTERPRI 'DEFINED-ON-LINE '62) 
(PUT 'PTERPRI 'DEFINED-IN-FILE 'GENTRAN/GOUTPUT.RED) 
(PUT 'PTERPRI 'PROCEDURE_TYPE '(ARROW UNIT GENERAL)) 
(DE PTERPRI NIL
    (PROG (CH PBUF)
      (SETQ CH (WRS NIL))
      (SETQ PBUF (REVERSIP *PPRINBUF*))
      (PROG (C)
        (SETQ C *OUTCHANL*)
       LAB
        (COND ((NULL C) (RETURN NIL)))
        ((LAMBDA (C)
           (PROGN
            (WRS C)
            (LINELENGTH 300)
            (PROG (A)
              (SETQ A PBUF)
             LAB
              (COND ((NULL A) (RETURN NIL)))
              ((LAMBDA (A)
                 (COND ((EQ GENTRANLANG* 'FORTRAN) (FPRIN2 A)) (T (PRIN2 A))))
               (CAR A))
              (SETQ A (CDR A))
              (GO LAB))
            (TERPRI)))
         (CAR C))
        (SETQ C (CDR C))
        (GO LAB))
      (SETQ *POSN* 0)
      (SETQ *PPRINBUF* NIL)
      (WRS CH))) 
(PUT 'GENTRANERR 'NUMBER-OF-ARGS 4) 
(PUT 'GENTRANERR 'DEFINED-ON-LINE '87) 
(PUT 'GENTRANERR 'DEFINED-IN-FILE 'GENTRAN/GOUTPUT.RED) 
(PUT 'GENTRANERR 'PROCEDURE_TYPE
     '(ARROW (TIMES GENERAL GENERAL GENERAL GENERAL) GENERAL)) 
(DE GENTRANERR (MSGTYPE EXP MSG1 MSG2)
    (PROG (HOLDICH HOLDOCH RESP)
      (SETQ HOLDICH (RDS *ERRCHAN*))
      (SETQ HOLDOCH (WRS *ERRCHAN*))
      (TERPRI)
      (COND (EXP (PRETTYPRINT EXP)))
      (COND
       ((AND (EQ MSGTYPE 'E) (NOT *ERRCONT))
        (PROGN (RDS (CDR *STDIN*)) (WRS (CDR *STDOUT*)) (REDERR MSG1))))
      (PRIN2 "*** ")
      (PRIN2T MSG1)
      (COND (MSG2 (SETQ RESP (YESP MSG2))))
      (WRS HOLDOCH)
      (RDS HOLDICH)
      (COND ((NOT (OR RESP *ERRCONT)) (ERROR1))))) 
(PUT 'MIN0 'NUMBER-OF-ARGS 2) 
(PUT 'MIN0 'DEFINED-ON-LINE '115) 
(PUT 'MIN0 'DEFINED-IN-FILE 'GENTRAN/GOUTPUT.RED) 
(PUT 'MIN0 'PROCEDURE_TYPE '(ARROW (TIMES GENERAL GENERAL) GENERAL)) 
(DE MIN0 (N1 N2) (MAX (MIN N1 N2) 0)) 
(PUT 'NSPACES 'NUMBER-OF-ARGS 1) 
(PUT 'NSPACES 'DEFINED-ON-LINE '118) 
(PUT 'NSPACES 'DEFINED-IN-FILE 'GENTRAN/GOUTPUT.RED) 
(PUT 'NSPACES 'PROCEDURE_TYPE '(ARROW GENERAL GENERAL)) 
(DE NSPACES (N)
    (PROG (S)
      (PROG (I)
        (SETQ I 1)
       LAB
        (COND ((MINUSP (DIFFERENCE N I)) (RETURN NIL)))
        (SETQ S (CONS '! (CONS BLANK S)))
        (SETQ I (PLUS2 I 1))
        (GO LAB))
      (RETURN (INTERN (COMPRESS S))))) 
(ENDMODULE) 