(cl:declaim (cl:optimize cl:debug cl:safety))
(cl:declaim (sb-ext:muffle-conditions sb-ext:compiler-note cl:style-warning))
(MODULE (LIST 'NCPOLY)) 
(CREATE-PACKAGE '(NCPOLY NCENV NCDIP NCGROEB NCFACTOR NCOUT) '(CONTRIB NCPOLY)) 
(FLUID
 '(BASECOUNT* BCOUNT* B4COUNT* DIPVARS* FACTORTIME* FCOUNT* GLTERMS*
   GROECONTCOUNT* GROETIME* HCOUNT* HZEROCOUNT* MCOUNT* NCCOF* NCDIPBASE*
   NCDIPCIRCULAR* NCDIPTABLE* NCDIPVARS* NCPI-BRACKETS* NCPI-COMM-RULES*
   NCPI-NAMES* NCPI-NAME-RULES* NC_FACTOR_TIME NC_FACTOR_TIME* VDPSORTMODE*
   PCOUNT* *EVALLHSEQP *GSUGAR *NCG-RIGHT *NC-TRAVERSO-SLOPPY *TRGROEB
   *TRGROEBS *TRGROEBFULL *TRNC *VAROPTT *VDPINTEGER)) 
(GLOBAL '(GLTERMS)) 
(SHARE (LIST 'NCPI-BRACKETS* 'NCPI-COMM-RULES* 'NCPI-NAME-RULES*)) 
(LOAD_PACKAGE (LIST 'DIPOLY)) 
(LOAD_PACKAGE (LIST 'GROEBNER)) 
(EXPORTS
 (LIST 'NC_CLEANUP 'NC_COMPACT 'LEFT_FACTOR 'LEFT_FACTORS 'NC_DIVIDE
       'NC_FACTORIZE 'NC_FACTORIZE_ALL 'NC_GROEBNER 'NC_PREDUCE 'NC_SETUP
       'RIGHT_FACTOR 'RIGHT_FACTORS)) 
((LAMBDA (V)
   (COND
    ((OR (NOT (NUMBERP V)) (LESSP V 2.8))
     (REDERR "Groebner package version too old"))))
 (GET 'GROEBNER 'VERSION)) 
(ENDMODULE) 