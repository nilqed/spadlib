(PUT 'DOMAIN-GCD 'DEFINED-IN-FILE 'MISC/COMFAC.RED) 
(PUT 'DOMAIN-GCD 'PROCEDURE_TYPE '(ARROW (TIMES GENERAL GENERAL) GENERAL)) 
(PUTC 'DOMAIN-GCD 'INLINE '(LAMBDA (U V) (GCDN U V))) 
(DE DOMAIN-ONEP (U) (ONEP U)) 
(PUT 'DOMAIN-ONEP 'NUMBER-OF-ARGS 1) 
(PUT 'DOMAIN-ONEP 'DEFINED-ON-LINE '34) 
(PUT 'DOMAIN-ONEP 'DEFINED-IN-FILE 'MISC/COMFAC.RED) 
(PUT 'DOMAIN-ONEP 'PROCEDURE_TYPE '(ARROW GENERAL GENERAL)) 
(PUTC 'DOMAIN-ONEP 'INLINE '(LAMBDA (U) (ONEP U))) 
(PUT 'MV-POW-ZEROP 'NUMBER-OF-ARGS 1) 
(PUT 'MV-POW-ZEROP 'DEFINED-ON-LINE '36) 
(PUT 'MV-POW-ZEROP 'DEFINED-IN-FILE 'MISC/COMFAC.RED) 
(PUT 'MV-POW-ZEROP 'PROCEDURE_TYPE '(ARROW GENERAL GENERAL)) 
(DE MV-POW-ZEROP (U) (OR (NULL U) (AND (ZEROP (CAR U)) (MV-POW-ZEROP (CDR U))))) 
(PUT 'MV-POW-GCD 'NUMBER-OF-ARGS 2) 
(PUT 'MV-POW-GCD 'DEFINED-ON-LINE '39) 
(PUT 'MV-POW-GCD 'DEFINED-IN-FILE 'MISC/COMFAC.RED) 
(PUT 'MV-POW-GCD 'PROCEDURE_TYPE '(ARROW (TIMES GENERAL GENERAL) GENERAL)) 
(DE MV-POW-GCD (U V)
    (COND ((NULL U) NIL)
          (T (CONS (MIN (CAR U) (CAR V)) (MV-POW-GCD (CDR U) (CDR V)))))) 
(PUT 'MV-CONTENT 'NUMBER-OF-ARGS 1) 
(PUT 'MV-CONTENT 'DEFINED-ON-LINE '43) 
(PUT 'MV-CONTENT 'DEFINED-IN-FILE 'MISC/COMFAC.RED) 
(PUT 'MV-CONTENT 'PROCEDURE_TYPE '(ARROW GENERAL GENERAL)) 
(DE MV-CONTENT (U)
    (COND ((NULL U) NIL)
          (T
           (PROG (X Y)
             (SETQ X (CDAR U))
             (SETQ Y (CAAR U))
            A
             (SETQ U (CDR U))
             (COND
              ((OR (NULL U) (AND (ONEP X) (MV-POW-ZEROP Y)))
               (RETURN (CONS Y X))))
             (SETQ X (GCDN X (CDAR U)))
             (SETQ Y (MV-POW-GCD Y (CAAR U)))
             (GO A))))) 
(ENDMODULE) 