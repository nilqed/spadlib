               (SETQ X ARGL)
               (COND ((NULL X) (RETURN NIL)))
               (SETQ FORALL-RESULT
                       (SETQ FORALL-ENDPTR
                               (CONS
                                ((LAMBDA (X) (CONS (CADR X) (CADDR X)))
                                 (CAR X))
                                NIL)))
              LOOPLABEL
               (SETQ X (CDR X))
               (COND ((NULL X) (RETURN FORALL-RESULT)))
               (RPLACD FORALL-ENDPTR
                       (CONS ((LAMBDA (X) (CONS (CADR X) (CADDR X))) (CAR X))
                             NIL))
               (SETQ FORALL-ENDPTR (CDR FORALL-ENDPTR))
               (GO LOOPLABEL)))
     (SETQ TALP_EXTLANG*
             (PROG (X FORALL-RESULT FORALL-ENDPTR)
               (SETQ X ARGL)
              STARTOVER
               (COND ((NULL X) (RETURN NIL)))
               (SETQ FORALL-RESULT
                       ((LAMBDA (X)
                          (COND
                           ((GREATERP (CADDR X) 0)
                            (CONS (CONS (CADR X) (CADDR X))
                                  (TALP_GET-INVS (CDR X))))
                           (T (LIST (CONS (CADR X) (CADDR X))))))
                        (CAR X)))
               (SETQ FORALL-ENDPTR (LASTPAIR FORALL-RESULT))
               (SETQ X (CDR X))
               (COND ((ATOM FORALL-ENDPTR) (GO STARTOVER)))
              LOOPLABEL
               (COND ((NULL X) (RETURN FORALL-RESULT)))
               (RPLACD FORALL-ENDPTR
                       ((LAMBDA (X)
                          (COND
                           ((GREATERP (CADDR X) 0)
                            (CONS (CONS (CADR X) (CADDR X))
                                  (TALP_GET-INVS (CDR X))))
                           (T (LIST (CONS (CADR X) (CADDR X))))))
                        (CAR X)))
               (SETQ FORALL-ENDPTR (LASTPAIR FORALL-ENDPTR))
               (SETQ X (CDR X))
               (GO LOOPLABEL)))
     (PROG (X)
       (SETQ X TALP_EXTLANG*)
      LAB
       (COND ((NULL X) (RETURN NIL)))
       ((LAMBDA (X) (COND ((GREATERP (CDR X) 0) (TALP_MKALOP X)))) (CAR X))
       (SETQ X (CDR X))
       (GO LAB))
     (CONS T ARGL))) 
