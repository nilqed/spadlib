(DE TALP_SIMPTERM (TERM)
    (PROG (ARITY OBJ)
      (COND ((ATOM TERM) (RETURN TERM)))
      (SETQ OBJ (ATSOC (TALP_OP TERM) TALP_EXTLANG*))
      (COND
       ((NULL OBJ) (REDERR (LIST (TALP_OP TERM) "not declared as operator"))))
      (SETQ ARITY (CDR OBJ))
      (COND
       ((NEQ (LENGTH (TALP_FARGL TERM)) ARITY)
        (REDERR (LIST (TALP_OP TERM) "  defined as  " ARITY "- ary")))
       (T
        (SETQ OBJ
                (PROG (ARG FORALL-RESULT FORALL-ENDPTR)
                  (SETQ ARG (TALP_FARGL TERM))
                  (COND ((NULL ARG) (RETURN NIL)))
                  (SETQ FORALL-RESULT
                          (SETQ FORALL-ENDPTR
                                  (CONS
                                   ((LAMBDA (ARG) (TALP_SIMPTERM ARG))
                                    (CAR ARG))
                                   NIL)))
                 LOOPLABEL
                  (SETQ ARG (CDR ARG))
                  (COND ((NULL ARG) (RETURN FORALL-RESULT)))
                  (RPLACD FORALL-ENDPTR
                          (CONS ((LAMBDA (ARG) (TALP_SIMPTERM ARG)) (CAR ARG))
                                NIL))
                  (SETQ FORALL-ENDPTR (CDR FORALL-ENDPTR))
                  (GO LOOPLABEL)))))
      (RETURN (TALP_MKN (TALP_OP TERM) OBJ)))) 
