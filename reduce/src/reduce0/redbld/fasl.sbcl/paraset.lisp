(cl:declaim (cl:optimize cl:debug cl:safety))
(cl:declaim (sb-ext:muffle-conditions sb-ext:compiler-note cl:style-warning))
(MODULE (LIST 'PARASET)) 
(IMPORTS (LIST 'ERRORP 'ERRORSET* 'NEQ 'ROUNDCONSTANTS)) 
(EXPORTS (LIST '!MFEFIX 'FIND!NBFPD 'FIND!NFPD 'INFINITYP)) 
(GLOBAL
 '(!NFPD !NBFPD !~XX !YY !MAXBFLEXP !FLEXPERR !PLUMAX !EPSQRT !FLINT !FLBINT
   !FLOATBITS)) 
(FLAG '(!NFPD !NBFPD !MAXFLEXP) 'SHARE) 
(PUT 'FIND!NFPD 'NUMBER-OF-ARGS 0) 
(PUT 'FIND!NFPD 'DEFINED-ON-LINE '52) 
(PUT 'FIND!NFPD 'DEFINED-IN-FILE 'ARITH/PARASET.RED) 
(PUT 'FIND!NFPD 'PROCEDURE_TYPE '(ARROW UNIT GENERAL)) 
(DE FIND!NFPD NIL
    (PROG (X Y I)
      (SETQ I 0)
      (SETQ X (SETQ Y 9.0))
      (PROG ()
       REPEATLABEL
        (PROGN (SETQ X (PLUS (TIMES 10.0 X) Y)) (SETQ I (PLUS I 1)))
        (COND ((NOT (EQUAL (PLUS X 1.0) X)) (GO REPEATLABEL))))
      (RETURN (SETQ !NFPD (PROGN (SETQ ALGLIST* (CONS NIL NIL)) I))))) 
(PUT 'FIND!NBFPD 'NUMBER-OF-ARGS 0) 
(PUT 'FIND!NBFPD 'DEFINED-ON-LINE '57) 
(PUT 'FIND!NBFPD 'DEFINED-IN-FILE 'ARITH/PARASET.RED) 
(PUT 'FIND!NBFPD 'PROCEDURE_TYPE '(ARROW UNIT GENERAL)) 
(DE FIND!NBFPD NIL
    (PROG (X Y I)
      (SETQ I 0)
      (SETQ X (SETQ Y 1.0))
      (PROG ()
       REPEATLABEL
        (PROGN (SETQ X (PLUS (TIMES 2.0 X) Y)) (SETQ I (PLUS I 1)))
        (COND ((NOT (EQUAL (PLUS X 1.0) X)) (GO REPEATLABEL))))
      (RETURN (SETQ !NBFPD (PROGN (SETQ ALGLIST* (CONS NIL NIL)) I))))) 
(PUT 'FIND!MAXBFLEXP 'NUMBER-OF-ARGS 0) 
(PUT 'FIND!MAXBFLEXP 'DEFINED-ON-LINE '62) 
(PUT 'FIND!MAXBFLEXP 'DEFINED-IN-FILE 'ARITH/PARASET.RED) 
(PUT 'FIND!MAXBFLEXP 'PROCEDURE_TYPE '(ARROW UNIT GENERAL)) 
(DE FIND!MAXBFLEXP NIL
    (PROG (Z N)
      (SETQ N 0)
      (SETQ !~XX 1.0)
      (PROG ()
       WHILELABEL
        (COND
         ((NOT
           (AND
            (NOT
             (ERRORP
              (SETQ Z
                      (ERRORSET*
                       '(PROGN
                         (SETQ !YY (PLUS 1.0 (TIMES !~XX 2.0)))
                         (AND (NOT (INFINITYP !YY)) (GREATERP !YY !~XX)))
                       NIL))))
            (CAR Z)))
          (RETURN NIL)))
        (PROGN (SETQ N (PLUS N 1)) (SETQ !~XX !YY))
        (GO WHILELABEL))
      (SETQ !FLEXPERR (AND (NOT (ERRORP Z)) (NOT (CAR Z))))
      (RETURN (SETQ !MAXBFLEXP N)))) 
(PUT 'FIND!MINNORM 'NUMBER-OF-ARGS 0) 
(PUT 'FIND!MINNORM 'DEFINED-ON-LINE '75) 
(PUT 'FIND!MINNORM 'DEFINED-IN-FILE 'ARITH/PARASET.RED) 
(PUT 'FIND!MINNORM 'PROCEDURE_TYPE '(ARROW UNIT GENERAL)) 
(DE FIND!MINNORM NIL
    (PROG (A B C)
      (SETQ A 1.0)
      (SETQ B 0.0)
      (PROG ()
       REPEATLABEL
        (PROGN (SETQ C B) (SETQ B A) (SETQ A (PLUS (TIMES 2.0 A) 1.0)))
        (COND ((NOT (EQUAL (QUOTIENT A 2.0) B)) (GO REPEATLABEL))))
      (PROG ()
       REPEATLABEL
        (PROGN (SETQ B C) (SETQ C (QUOTIENT C 2.0)))
        (COND ((NOT (NEQ (TIMES 2.0 C) B)) (GO REPEATLABEL))))
      (COND ((EQUAL C 0.0) (SETQ !MINNORM B)) (T (SETQ !MINNORM C)))
      (SETQ !MINNEGNORM (MINUS !MINNORM)))) 
(PUT 'INFINITYP 'NUMBER-OF-ARGS 1) 
(PUT 'INFINITYP 'DEFINED-ON-LINE '97) 
(PUT 'INFINITYP 'DEFINED-IN-FILE 'ARITH/PARASET.RED) 
(PUT 'INFINITYP 'PROCEDURE_TYPE '(ARROW GENERAL GENERAL)) 
(DE INFINITYP (U)
    ((LAMBDA (X) (NOT (OR (EQ X '-) (DIGIT X)))) (CAR (EXPLODE U)))) 
(PUT '!MFEFIX 'NUMBER-OF-ARGS 0) 
(PUT '!MFEFIX 'DEFINED-ON-LINE '102) 
(PUT '!MFEFIX 'DEFINED-IN-FILE 'ARITH/PARASET.RED) 
(PUT '!MFEFIX 'PROCEDURE_TYPE '(ARROW UNIT GENERAL)) 
(DE !MFEFIX NIL
    (PROGN
     (COND
      (!FLEXPERR
       (PROG (N)
         (SETQ N 0)
         (SETQ !FLEXPERR (SETQ !PLUMAX NIL))
         (PROG ()
          WHILELABEL
           (COND ((NOT (ERRORP (ERRORSET* '(EXPLODE !~XX) NIL))) (RETURN NIL)))
           (PROGN (SETQ !~XX (QUOTIENT !~XX 2.0)) (SETQ N (PLUS N 1)))
           (GO WHILELABEL))
         (SETQ !MAXBFLEXP (DIFFERENCE !MAXBFLEXP N)))))
     (COND ((NOT !PLUMAX) (ROUNDCONSTANTS))))) 
(FIND!NFPD) 
(FIND!NBFPD) 
(FIND!MAXBFLEXP) 
(FIND!MINNORM) 
(SETQ !EPSQRT (EXPT 10.0 (QUOTIENT (DIFFERENCE (MINUS 1) !NFPD) 2))) 
(SETQ !FLINT (EXPT 10.0 !NFPD)) 
(SETQ !FLBINT (EXPT 2.0 !NBFPD)) 
(SETQ !FLOATBITS (QUOTIENT (TIMES 10 (PLUS !NFPD 1)) 3)) 
(DE FP-INFINITE (X)
    (AND (OR (GREATERP X 1.0e300) (LESSP X (MINUS 1.0e300)))
         (EQN (QUOTIENT 1.0 X) 0.0))) 
(PUT 'FP-INFINITE 'NUMBER-OF-ARGS 1) 
(PUT 'FP-INFINITE 'DEFINED-ON-LINE '140) 
(PUT 'FP-INFINITE 'DEFINED-IN-FILE 'ARITH/PARASET.RED) 
(PUT 'FP-INFINITE 'PROCEDURE_TYPE '(ARROW GENERAL GENERAL)) 
(PUTC 'FP-INFINITE 'INLINE
      '(LAMBDA (X)
         (AND (OR (GREATERP X 1.0e300) (LESSP X (MINUS 1.0e300)))
              (EQN (QUOTIENT 1.0 X) 0.0)))) 
(DE FP-NAN (X) (NOT (EQN X X))) 
(PUT 'FP-NAN 'NUMBER-OF-ARGS 1) 
(PUT 'FP-NAN 'DEFINED-ON-LINE '143) 
(PUT 'FP-NAN 'DEFINED-IN-FILE 'ARITH/PARASET.RED) 
(PUT 'FP-NAN 'PROCEDURE_TYPE '(ARROW GENERAL GENERAL)) 
(PUTC 'FP-NAN 'INLINE '(LAMBDA (X) (NOT (EQN X X)))) 
(DE FP-FINITE (X) (EQN (DIFFERENCE X X) 0.0)) 
(PUT 'FP-FINITE 'NUMBER-OF-ARGS 1) 
(PUT 'FP-FINITE 'DEFINED-ON-LINE '146) 
(PUT 'FP-FINITE 'DEFINED-IN-FILE 'ARITH/PARASET.RED) 
(PUT 'FP-FINITE 'PROCEDURE_TYPE '(ARROW GENERAL GENERAL)) 
(PUTC 'FP-FINITE 'INLINE '(LAMBDA (X) (EQN (DIFFERENCE X X) 0.0))) 
(DE FP-SUBNORM (X) (AND (LESSP X !MINNORM) (GREATERP X !MINNEGNORM))) 
(PUT 'FP-SUBNORM 'NUMBER-OF-ARGS 1) 
(PUT 'FP-SUBNORM 'DEFINED-ON-LINE '149) 
(PUT 'FP-SUBNORM 'DEFINED-IN-FILE 'ARITH/PARASET.RED) 
(PUT 'FP-SUBNORM 'PROCEDURE_TYPE '(ARROW GENERAL GENERAL)) 
(PUTC 'FP-SUBNORM 'INLINE
      '(LAMBDA (X) (AND (LESSP X !MINNORM) (GREATERP X !MINNEGNORM)))) 
(ENDMODULE) 