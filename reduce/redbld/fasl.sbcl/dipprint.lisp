(cl:declaim (cl:optimize cl:debug cl:safety))
(cl:declaim (sb-ext:muffle-conditions sb-ext:compiler-note cl:style-warning))
(MODULE (LIST 'DIPPRINT)) 
(PUT 'DIPLPRINT 'NUMBER-OF-ARGS 1) 
(PUT 'DIPLPRINT 'DEFINED-ON-LINE '30) 
(PUT 'DIPLPRINT 'DEFINED-IN-FILE 'DIPOLY/DIPPRINT.RED) 
(PUT 'DIPLPRINT 'PROCEDURE_TYPE '(ARROW GENERAL GENERAL)) 
(DE DIPLPRINT (U)
    (PROG (V)
      (SETQ V U)
     LAB
      (COND ((NULL V) (RETURN NIL)))
      ((LAMBDA (V) (DIPPRINT V)) (CAR V))
      (SETQ V (CDR V))
      (GO LAB))) 
(PUT 'DIPPRINT 'NUMBER-OF-ARGS 1) 
(PUT 'DIPPRINT 'DEFINED-ON-LINE '34) 
(PUT 'DIPPRINT 'DEFINED-IN-FILE 'DIPOLY/DIPPRINT.RED) 
(PUT 'DIPPRINT 'PROCEDURE_TYPE '(ARROW GENERAL GENERAL)) 
(DE DIPPRINT (U) (PROGN (TERPRI) (DIPPRINT1 U NIL) (TERPRI) (TERPRI))) 
(PUT 'DIPPRINT1 'NUMBER-OF-ARGS 2) 
(PUT 'DIPPRINT1 'DEFINED-ON-LINE '38) 
(PUT 'DIPPRINT1 'DEFINED-IN-FILE 'DIPOLY/DIPPRINT.RED) 
(PUT 'DIPPRINT1 'PROCEDURE_TYPE '(ARROW (TIMES GENERAL GENERAL) GENERAL)) 
(DE DIPPRINT1 (U V)
    (COND ((NULL U) (COND ((NULL V) (DIPPRIN2 0)) (T NIL)))
          (T
           (PROG (BOOL W)
             (SETQ W (CADR U))
             (COND ((BCMINUS? W) (PROGN (SETQ BOOL T) (SETQ W (BCNEG W)))))
             (COND (BOOL (DIPPRIN2 " - ")) (V (DIPPRIN2 " + ")))
             ((LAMBDA (X)
                (COND
                 ((OR (NOT (BCONE? W)) (EVZERO? X))
                  (PROGN (BCPRIN W) (DIPEVLPRI X T)))
                 (T (DIPEVLPRI X NIL))))
              (CAR U))
             (DIPPRINT1 (CDDR U) T))))) 
(PUT 'DIPPRIN2 'NUMBER-OF-ARGS 1) 
(PUT 'DIPPRIN2 'DEFINED-ON-LINE '52) 
(PUT 'DIPPRIN2 'DEFINED-IN-FILE 'DIPOLY/DIPPRINT.RED) 
(PUT 'DIPPRIN2 'PROCEDURE_TYPE '(ARROW GENERAL GENERAL)) 
(DE DIPPRIN2 (U)
    (PROGN (COND ((GREATERP (POSN) 69) (PROGN (TERPRI) (TERPRI)))) (PRIN2 U))) 
(ENDMODULE) 