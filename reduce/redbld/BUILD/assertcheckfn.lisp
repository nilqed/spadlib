(PUT 'SFPX1 'NUMBER-OF-ARGS 5) 
(PUT 'SFPX1 'DEFINED-ON-LINE '134) 
(PUT 'SFPX1 'DEFINED-IN-FILE 'ASSERT/ASSERTCHECKFN.RED) 
(PUT 'SFPX1 'PROCEDURE_TYPE
     '(ARROW (TIMES GENERAL GENERAL GENERAL GENERAL GENERAL) GENERAL)) 
(DE SFPX1 (U VL V D CHKORD)
    (PROG (C L P R VV DD)
      (SETQ DD 0)
      (COND ((OR (ATOM U) (ATOM (CAR U))) (RETURN T)))
      (COND ((NOT (PAIRP U)) (RETURN NIL)))
      (SETQ L (CAR U))
      (SETQ R (CDR U))
      (COND ((NOT (PAIRP L)) (RETURN NIL)))
      (SETQ C (CDR L))
      (SETQ P (CAR L))
      (COND ((NOT (PAIRP P)) (RETURN NIL)))
      (SETQ VV (CAR P))
      (COND ((NOT (ASSERT_KERNELP VV)) (RETURN NIL)))
      (SETQ DD (CDR P))
      (COND
       ((EQ VV V)
        (RETURN
         (AND (LESSP DD D) (SFPX1 C (CONS V VL) NIL 0 CHKORD)
              (SFPX1 R VL V DD CHKORD)))))
      (COND (V (SETQ VL (CONS V VL))))
      (COND
       ((AND CHKORD VL (ORDPP (CONS VV 1) (CONS (CAR VL) 1))) (RETURN NIL)))
      (RETURN
       (AND (SFPX1 C (CONS VV VL) NIL 0 CHKORD) (SFPX1 R VL VV DD CHKORD))))) 
