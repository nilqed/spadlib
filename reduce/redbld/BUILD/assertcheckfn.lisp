(PUT 'ASSERT_KERNELP 'DEFINED-IN-FILE 'ASSERT/ASSERTCHECKFN.RED) 
(PUT 'ASSERT_KERNELP 'PROCEDURE_TYPE '(ARROW GENERAL GENERAL)) 
(DE ASSERT_KERNELP (U)
    (PROG (W)
      (COND ((IDP U) (RETURN T)))
      (COND ((NOT (PAIRP U)) (RETURN NIL)))
      (COND ((GET (CAR U) 'FKERNFN) (RETURN T)))
      (SETQ W (COND ((ATOM (CAR U)) (GET (CAR U) 'KLIST)) (T EXLIST*)))
      (RETURN (ATSOC U W)))) 
