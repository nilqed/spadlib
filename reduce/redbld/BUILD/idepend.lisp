(cl:declaim (cl:optimize cl:debug cl:safety))
(cl:declaim (sb-ext:muffle-conditions sb-ext:compiler-note cl:style-warning))
(MODULE (LIST 'IDEPEND)) 
(FLUID '(TAYLORVARIABLE)) 
(EXPORTS (LIST 'DEPENDSPL 'DEPENDSP 'INVOLVESQ 'INVOLVSF)) 
(IMPORTS (LIST 'DOMAINP)) 
(PUT 'DEPENDSP 'NUMBER-OF-ARGS 2) 
(PUT 'DEPENDSP 'DEFINED-ON-LINE '36) 
(PUT 'DEPENDSP 'DEFINED-IN-FILE 'INT/IDEPEND.RED) 
(PUT 'DEPENDSP 'PROCEDURE_TYPE '(ARROW (TIMES GENERAL GENERAL) GENERAL)) 
(DE DEPENDSP (X V)
    (COND ((NULL V) T) ((DEPENDS X V) X) ((ATOM X) (COND ((EQ X V) X) (T NIL)))
          ((EQUAL (CAR X) '*SQ) (INVOLVESQ (CADR X) V))
          ((TAYLORP X) (COND ((EQ V TAYLORVARIABLE) TAYLORVARIABLE) (T NIL)))
          (T
           (PROG (W)
             (COND ((EQUAL X V) (RETURN V)))
             (SETQ W X)
             (SETQ X (CDR X))
            SCAN
             (COND ((NULL X) (RETURN NIL)))
             (COND ((DEPENDSP (CAR X) V) (RETURN W)))
             (SETQ X (CDR X))
             (GO SCAN))))) 
(PUT 'INVOLVESQ 'NUMBER-OF-ARGS 2) 
(PUT 'INVOLVESQ 'DEFINED-ON-LINE '55) 
(PUT 'INVOLVESQ 'DEFINED-IN-FILE 'INT/IDEPEND.RED) 
(PUT 'INVOLVESQ 'PROCEDURE_TYPE '(ARROW (TIMES GENERAL GENERAL) GENERAL)) 
(DE INVOLVESQ (SQ TERM)
    (OR (INVOLVESF (CAR SQ) TERM) (INVOLVESF (CDR SQ) TERM))) 
(PUT 'INVOLVESF 'NUMBER-OF-ARGS 2) 
(PUT 'INVOLVESF 'DEFINED-ON-LINE '58) 
(PUT 'INVOLVESF 'DEFINED-IN-FILE 'INT/IDEPEND.RED) 
(PUT 'INVOLVESF 'PROCEDURE_TYPE '(ARROW (TIMES GENERAL GENERAL) GENERAL)) 
(DE INVOLVESF (SF TERM)
    (COND ((OR (OR (ATOM SF) (ATOM (CAR SF))) (NULL SF)) NIL)
          (T
           (OR (DEPENDSP (CAAAR SF) TERM) (INVOLVESF (CDAR SF) TERM)
               (INVOLVESF (CDR SF) TERM))))) 
(PUT 'DEPENDSPL 'NUMBER-OF-ARGS 2) 
(PUT 'DEPENDSPL 'DEFINED-ON-LINE '64) 
(PUT 'DEPENDSPL 'DEFINED-IN-FILE 'INT/IDEPEND.RED) 
(PUT 'DEPENDSPL 'PROCEDURE_TYPE '(ARROW (TIMES GENERAL GENERAL) GENERAL)) 
(DE DEPENDSPL (DEP-LIST VAR)
    (AND DEP-LIST
         (OR (DEPENDSP (CAR DEP-LIST) VAR) (DEPENDSPL (CDR DEP-LIST) VAR)))) 
(DE TAYLORFUNCTION (U) (CAAR U)) 
(PUT 'TAYLORFUNCTION 'NUMBER-OF-ARGS 1) 
(PUT 'TAYLORFUNCTION 'DEFINED-ON-LINE '70) 
(PUT 'TAYLORFUNCTION 'DEFINED-IN-FILE 'INT/IDEPEND.RED) 
(PUT 'TAYLORFUNCTION 'PROCEDURE_TYPE '(ARROW GENERAL GENERAL)) 
(PUTC 'TAYLORFUNCTION 'INLINE '(LAMBDA (U) (CAAR U))) 
(PUT 'TAYLORP 'NUMBER-OF-ARGS 1) 
(PUT 'TAYLORP 'DEFINED-ON-LINE '72) 
(PUT 'TAYLORP 'DEFINED-IN-FILE 'INT/IDEPEND.RED) 
(PUT 'TAYLORP 'PROCEDURE_TYPE '(ARROW GENERAL GENERAL)) 
(DE TAYLORP (EXXPR)
    (AND (NOT (ATOM EXXPR)) (NOT (ATOM (CAR EXXPR)))
         (FLAGP (CAAR EXXPR) 'TAYLOR))) 
(ENDMODULE) 