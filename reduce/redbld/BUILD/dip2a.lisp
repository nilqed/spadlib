(cl:declaim (cl:optimize cl:debug cl:safety))
(cl:declaim (sb-ext:muffle-conditions sb-ext:compiler-note cl:style-warning))
(MODULE (LIST 'DIP2A)) 
(PUT 'DIP2A 'NUMBER-OF-ARGS 1) 
(PUT 'DIP2A 'DEFINED-ON-LINE '32) 
(PUT 'DIP2A 'DEFINED-IN-FILE 'DIPOLY/DIP2A.RED) 
(PUT 'DIP2A 'PROCEDURE_TYPE '(ARROW GENERAL GENERAL)) 
(DE DIP2A (U) (COND ((NULL U) 0) (T (DIPREPLUS (DIP2A1 U))))) 
(PUT 'DIP2A1 'NUMBER-OF-ARGS 1) 
(PUT 'DIP2A1 'DEFINED-ON-LINE '36) 
(PUT 'DIP2A1 'DEFINED-IN-FILE 'DIPOLY/DIP2A.RED) 
(PUT 'DIP2A1 'PROCEDURE_TYPE '(ARROW GENERAL GENERAL)) 
(DE DIP2A1 (U)
    (COND ((NULL U) NIL)
          (T
           (CONS
            ((LAMBDA (X Y)
               (COND
                ((BCMINUS? X)
                 (LIST 'MINUS (DIPRETIMES (CONS (BC2A (BCNEG X)) Y))))
                (T (DIPRETIMES (CONS (BC2A X) Y)))))
             (CADR U) (EXPVEC2A (CAR U)))
            (DIP2A1 (CDDR U)))))) 
(PUT 'DIPREPLUS 'NUMBER-OF-ARGS 1) 
(PUT 'DIPREPLUS 'DEFINED-ON-LINE '43) 
(PUT 'DIPREPLUS 'DEFINED-IN-FILE 'DIPOLY/DIP2A.RED) 
(PUT 'DIPREPLUS 'PROCEDURE_TYPE '(ARROW GENERAL GENERAL)) 
(DE DIPREPLUS (U)
    (COND ((ATOM U) U) ((NULL (CDR U)) (CAR U)) (T (CONS 'PLUS U)))) 
(PUT 'DIPRETIMES 'NUMBER-OF-ARGS 1) 
(PUT 'DIPRETIMES 'DEFINED-ON-LINE '46) 
(PUT 'DIPRETIMES 'DEFINED-IN-FILE 'DIPOLY/DIP2A.RED) 
(PUT 'DIPRETIMES 'PROCEDURE_TYPE '(ARROW GENERAL GENERAL)) 
(DE DIPRETIMES (U)
    (COND ((EQUAL (CAR U) 1) (COND ((CDR U) (DIPRETIMES (CDR U))) (T 1)))
          ((NULL (CDR U)) (CAR U)) (T (CONS 'TIMES U)))) 
(ENDMODULE) 